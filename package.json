{
  "name": "@ctrl/transmission",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for transmission using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/transmission",
  "keywords": [
    "transmission",
    "typescript"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:ci": "jest --coverage --no-cache --runInBand",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@ctrl/shared-torrent": "^1.3.2",
    "got": "^9.6.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@types/got": "9.6.9",
    "@types/jest": "24.0.25",
    "@types/node": "12.12.22",
    "@types/url-join": "4.0.0",
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "eslint": "6.8.0",
    "eslint-config-xo-space": "0.22.0",
    "eslint-config-xo-typescript": "0.23.0",
    "eslint-plugin-import": "2.19.1",
    "jest": "24.9.0",
    "p-wait-for": "3.1.0",
    "semantic-release": "15.14.0",
    "ts-jest": "24.2.0",
    "typedoc": "0.15.5",
    "typescript": "3.7.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
